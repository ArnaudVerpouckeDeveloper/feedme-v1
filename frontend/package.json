{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build && npm run copyIndexBladeFile && npm run removeGeneratedIndexFile",
        "copyIndexBladeFile": "copyfiles ./index.blade.php ../feedme/public/spa",
        "removeGeneratedIndexFile": "del-cli --force ../feedme/public/spa/index.html"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "core-js": "^2.6.5",
        "move-file-cli": "^1.0.0",
        "vue": "^2.6.10",
        "vue-router": "^3.0.3",
        "vuetify": "^2.2.11",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "copyfiles": "^2.2.0",
        "@vue/cli-plugin-babel": "^3.11.0",
        "@vue/cli-service": "^3.11.0",
        "del-cli": "^3.0.0",
        "sass": "^1.19.0",
        "sass-loader": "^8.0.0",
        "vue-cli-plugin-vuetify": "^2.0.5",
        "vue-template-compiler": "^2.6.10",
        "vuetify-loader": "^1.3.0"
    }
}